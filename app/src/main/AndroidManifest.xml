<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kyrostechnologies.sample.mailbox" >

    <application
        android:name="com.kyrostechnologies.sample.mailbox.data.GlobalVariable"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kyrostechnologies.sample.mailbox.ActivityMain"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kyrostechnologies.sample.mailbox.ActivityMailDetails" >
        </activity>
        <activity android:name="com.kyrostechnologies.sample.mailbox.ActivityComposeMail" >
        </activity>
    </application>

</manifest>
